#include <stdio.h>

int main(){
    printf("%s\n","Hello World");
    printf("%d\n",53);
    //%d is format specifier for whole numbers
    printf("%f\n",3.14);
    //%f is format specifier for outputting whole numbers
    printf("%c\n",'X');
    //%c is format specifier for outputting single characters
    //\n is used for creating new lines in ur C programme
    return 0;
}
